{
  "root": "build/",
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
      "Content-Security-Policy": "script-src 'self'",
      "X-XSS-Protection": "1; mode=block",
      "Cache-Control": "no-cache; max-age=0"
    },
    "/service-worker.js": {
      "Content-Security-Policy": "script-src 'self' https://storage.googleapis.com"
    },
    "/developers/**": {
      "Content-Security-Policy": "script-src 'self' https://buttons.github.io"
    }
  },
  "proxies": {
    "/api/": {
      "origin": "${API_HOST}"
    }
  },
  "routes": {
    "/static/**": "/static/",
    "/developers/playground": "api-playground.html",
    "/developers/**": "/developers/",
    "/**": "index.html"
  },
  "redirects": {
    "/developers": {
      "url": "/developers/docs/intro.html"
    },
    "/developers/": {
      "url": "/developers/docs/intro.html"
    }
  }
}
