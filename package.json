    "@mui/system": "^5.15.0",
    "@mui/x-date-pickers": "^6.19.0",
    "@sentry/browser": "7.13.0",
    "@sentry/react": "7.13.0",
    "@svgr/webpack": "^6.0.0",
    "@typeform/embed-react": "^2.30.0",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-preset-react-app": "^10.0.1",
    "broadcast-channel": "^7.1.0",
