    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "classnames": "^2.2.6",
    "common": "0.1.0",
    "core-js": "^3.8.1",
    "crisp-sdk-web": "^1.0.18",
    "css-loader": "^6.0.0",
    "csv-stringify": "^5.3.6",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^8.1.0",
    "gh-pages": "^6.2.0",
    "graphiql": "^3.0.0",
    "graphql": "^15.0.0",
    "html-webpack-plugin": "^5.6.0",
    "http-proxy-middleware": "^3.0.5",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-resolve": "^26.5.0",
    "jest-watch-typeahead": "^0.5.0",
    "libphonenumber-js": "^1.11.1",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^2.9.0",
    "moment": "^2.29.4",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss": "^8.4.0",
    "postcss-flexbugs-fixes": "^5.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-normalize": "^10.0.0",
    "postcss-preset-env": "^9.1.4",
    "postcss-safe-parser": "^5.0.0",
    "process": "^0.11.10",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^11.3.1",
    "react-error-boundary": "^3.1.3",
    "react-minimal-pie-chart": "^8.2.0",
    "react-multi-carousel": "^2.8.2",
    "react-multi-date-picker": "^4.5.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.3.3",
    "react-syntax-highlighter": "^15.6.1",
    "react-virtualized": "^9.22.2",
    "react-world-flags": "^1.6.0",
    "recharts": "^2.15.4",
    "reselect": "^4.0.0",
    "resolve": "^1.12.2",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.0.0",
    "semver": "^7.5.4",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-pnp": "^1.1.5",
    "typescript": "^4.9.5",
    "webpack": "^5.94.0",
    "webpack-dev-server": "5.0.0",
    "webpack-manifest-plugin": "^5.0.1",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node scripts/start.js",
    "start-clean": "rm -rf node_modules/.cache && node scripts/start.js",
    "build": "node scripts/build.js",
    "build-analyze": "INLINE_RUNTIME_CHUNK=false node scripts/build.js --analyze",
    "build-with-nginx": "yarn build && scripts/start_nginx.sh",
    "test": "node scripts/test.js --passWithNoTests",
    "lint": "NODE_ENV=development eslint --config eslint.config.mjs --ext .js,.jsx ./",
    "postinstall": "react-dsfr copy-static-assets",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "engines": {
    "node": ">=16",
    "yarn": ">=1.19 <2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not op_mob >=1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,json,md,mdx,css,yaml}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx,mjs}": [
      "./scripts/lint-fix.sh"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@cfaester/enzyme-adapter-react-18": "^0.8.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-webpack-plugin": "^5.0.2",
    "globals": "^15.12.0",
    "graceful-fs": "^4.2.10",
    "jest-environment-jsdom": "^30.1.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.0.7",
    "marked": "^4.1.1",
    "path-browserify": "^1.0.1",
    "postcss-custom-properties": "^14.0.6",
    "prettier": "^3.3.3",
    "react-refresh": "^0.17.0",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.55.0",
    "url": "^0.11.4",
    "url-loader": "^4.1.0",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/web",
      "<rootDir>/common"
    ],
    "collectCoverageFrom": [
      "web/**/*.{js,jsx,ts,tsx}",
      "!web/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/web/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/web/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/web/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/common/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/common/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@codegouvfr/react-dsfr)",
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
