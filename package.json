{
  "name": "mobilic",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "web",
      "common",
      "playground"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/runtime": "^7.6.2",
    "@dataesr/react-dsfr": "^2.2.2",
