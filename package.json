    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-plugin-recharts": "^2.0.0",
    "babel-preset-react-app": "^9.1.0",
    "broadcast-channel": "^3.2.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "classnames": "^2.2.6",
    "common": "0.1.0",
    "core-js": "^3.8.1",
    "css-loader": "^3.2.0",
    "csv-stringify": "^5.3.6",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^4.3.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^8.1.0",
    "graphiql": "^2.0.9",
    "graphql": "^15.0.0",
    "html-webpack-plugin": "^4.5.1",
    "http-proxy-middleware": "^1.0.5",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.0.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-resolve": "^25.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.29.4",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^1.0.5",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.1",
    "react-error-boundary": "^3.1.3",
    "react-minimal-pie-chart": "^8.2.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^17.4.0",
    "react-virtualized": "^9.22.2",
    "recharts": "^2.1.5",
    "reselect": "^4.0.0",
    "resolve": "^1.12.2",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "semver": "^6.3.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "^2.3.6",
    "ts-pnp": "^1.1.5",
    "typescript": "^3.7.5",
    "url-loader": "^2.3.0",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node scripts/start.js",
    "start-https": "HTTPS=true node scripts/start.js",
    "build": "INLINE_RUNTIME_CHUNK=false node scripts/build.js && rm build/static/js/*.map",
    "build-analyze": "INLINE_RUNTIME_CHUNK=false node scripts/build.js --analyze",
    "build-with-nginx": "yarn build && scripts/start_nginx.sh",
    "test": "node scripts/test.js",
    "lint": "eslint --ext .js,.jsx ./"
  },
  "engines": {
    "node": "14",
    "yarn": ">=1.19 <2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,json,md,mdx,css,yaml}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@sentry/webpack-plugin": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graceful-fs": "^4.2.10",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.0.7",
    "marked": "^4.1.1",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.55.0",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/web",
      "<rootDir>/common"
    ],
    "collectCoverageFrom": [
      "web/**/*.{js,jsx,ts,tsx}",
      "!web/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/web/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/web/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/web/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/common/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/common/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
